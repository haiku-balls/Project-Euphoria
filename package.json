{
  "name": "project-cutie",
  "author": "Baka",
  "license": "ISC",
  "version": "0.4.1",
  "description": "A puzzle game, now in electron!",
  "main": "index.js",
  "build": {
    "win": {
      "target": "squirrel"
    }
  },
  "updater": {
    "url": "https://raw.githubusercontent.com/user/example/master/updates/{platform}-{arch}-{channel}.json"
  },
  "scripts": {
    "start": "electron .",
    "chrome": "electron chrome.js",
    "update": "npm install electron-nightly@latest && npm update && npx npm-check-updates -u",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows"
  },
  "devDependencies": {
    "electron-builder": "^22.14.5",
    "electron-simple-publisher": "^2.0.1",
    "electron-builder-squirrel-windows": "^23.0.0-alpha.0"
  },
  "dependencies": {
    "ajv": "^8.9.0",
    "discord-rpc": "^4.0.1",
    "electron-is": "^3.0.0",
    "electron-nightly": "^18.0.0-nightly.20220119",
    "electron-simple-updater": "^2.0.11",
    "electron-store": "^8.0.1",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.17.2",
    "sweetalert": "^2.1.2"
  }
}
