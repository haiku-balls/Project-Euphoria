{
  "name": "project-cutie",
  "build": {
    "productName": "Project Cutie Electron",
    "appId": "com.electron.${name}",
    "category": "public.app-category.games",
    "darkModeSupport": "true",
    "type": "development",
    "copyright": "${author} 2021",
    "output": "int-0.4.1"
  },
  "version": "0.4.1",
  "description": "A puzzle game, now in electron!",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --windows",
    "release": "electron-builder --windows --publish always"
  },
  "author": "Baka",
  "license": "ISC",
  "devDependencies": {
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "ajv": "^8.8.2",
    "discord-rpc": "^4.0.1",
    "electron-browser-storage": "^1.0.7",
    "electron-nightly": "^18.0.0-nightly.20211119",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.17.2"
  }
}
